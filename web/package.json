{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start2": "ng serve --host 192.168.1.14 --port 4200",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build",
    "test": "ng test",
    "tst-cov": "ng test --code-coverage",
    "tst-nw": "ng test --no-watch",
    "tst-nw-cov": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --configuration production",
    "prodnaot": "ng build --configuration production --aot=false --buildOptimizer=false --vendorChunk=true",
    "proddev": "ng build --configuration production --output-path=distdev --base-href=/rt/",
    "dev": "ng build --configuration production --output-path=dev",
    "server": "http-server -p 4200 -c-1 dist/",
    "build-serve": "npm run prod && npm run server"
  },
  "private": true,
